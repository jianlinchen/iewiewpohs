#import "Product.h"

@implementation Product
-(void)getProduct:(NSDictionary *)p{
    Dictionary2Object(p, @"id", self.id);
    Dictionary2Object(p, @"price", self.price);
    Dictionary2Object(p, @"productName", self.productName);
    Dictionary2Object(p, @"imgUrl", self.imgUrl);
    Dictionary2Object(p, @"detail", self.detail);
    Dictionary2Object(p, @"brand", self.brand);
    
    Dictionary2Object(p, @"standard", self.standard);
    Dictionary2Object(p, @"exchangeFlag", self.exchangeFlag);
    Dictionary2Object(p, @"exchangePoint", self.exchangePoint);
    Dictionary2Object(p, @"status", self.status);
    
    Dictionary2Object(p, @"statusName", self.statusName);
    Dictionary2Object(p, @"shopName", self.shopName);
    Dictionary2Object(p, @"collectCount", self.collectCount);
    Dictionary2Object(p, @"wapUrl", self.wapUrl);

}

@end
